<h1>Books</h1>
  <table>
    <tr>
     <th>Title</th>
     <th>Body</th>
     <th colspan="3"></th>
    </tr>
　　<% @book.each do |book|%>
    <tr align="center">
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to "show", book_path(book) %></td>
      <td><%= link_to "edit", edit_book_path(book) %></td>
      <td><%= link_to "delete", book_path(book), method: :delete %></td>
    </tr>
　<% end %>

