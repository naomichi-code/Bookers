<h2></h2>
<%= @book.each do |book|%>
  <table>
    <tr>
     <th>Title</th>
     <th>Body</th>
     <th>""</th>
    </tr>
    <tr>
      <td><%= @blog.title %></td>
      <td><%= @blog.body %></td>
    </tr>
    <tr>
      <td><%= link_to "show", book_path(book) %></td>
      <td><%= link_to "edit", edit_book_path(book) %></td>
      <td><%= link_to "delete", book_path(book), method: :delete %></td>
    </tr>
  </table>
<%= end %>